{
  "index": "{{index}}",
  "scroll": "30s",
  "search_type": "scan",
  "size" : 100,
  "body":{
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "@timestamp": {
              "gt": "{{lastMessage}}",
              "to": "now"
            }
          }
        }
      ],
"must_not": [
{
"constant_score": {
"filter": {
"missing": {
"field": "message"
}
}
}
}
,
{
"constant_score": {
"filter": {
"missing": {
"field": "@timestamp"
}
}
}
}
],
      "should": []
    }
  },
	  "sort": {
    "@timestamp": {
      "order": "asc"
    }
  }
}
}

